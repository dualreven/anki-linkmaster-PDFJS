{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "docs/contracts/pdf-library/v1/messages/remove.completed.schema.json",
  "title": "pdf-library:remove:completed",
  "type": "object",
  "properties": {
    "type": {"const": "pdf-library:remove:completed"},
    "timestamp": {"type": "number"},
    "request_id": {"type": "string"},
    "status": {"const": "success"},
    "code": {"type": "integer"},
    "message": {"type": "string"},
    "metadata": {"type": "object", "properties": {"version": {"type": "string"}}, "required": ["version"], "additionalProperties": true},
    "data": {
      "type": "object",
      "properties": {
        "removed_files": {"type": "array", "items": {"type": "string"}},
        "failed_files": {"type": "object"},
        "total_removed": {"type": "integer"},
        "total_failed": {"type": "integer"}
      },
      "required": ["removed_files"],
      "additionalProperties": true
    }
  },
  "required": ["type", "timestamp", "request_id", "status", "code", "data", "metadata"],
  "additionalProperties": false
}
