{
  "name": "anki-linkmaster-pdfjs",
  "version": "1.0.0",
  "description": "Anki LinkMaster PDFJS - PDF management and viewer application",
  "type": "module",
  "scripts": {
    "preinstall": "node scripts/only-allow.js",
    "dev": "vite",
    "build": "vite build",
    "build:pdf-home": "vite build --outDir dist/pdf-home --config vite.config.js --base=/pdf-home/",
    "build:pdf-viewer": "vite build --outDir dist/pdf-viewer --config vite.config.js --base=/pdf-viewer/",
    "build:all": "pnpm run build:pdf-home && pnpm run build:pdf-viewer",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "benchmark": "node AItemp/performance-benchmark.js"
  },
  "engines": {
    "node": ">=14.0.0",
    "pnpm": ">=7.0.0"
  },
  "packageManager": "pnpm@10.17.1",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/plugin-proposal-class-properties": "^7.18.0",
    "@babel/plugin-proposal-private-methods": "^7.18.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.27.1",
    "@babel/plugin-transform-optional-chaining": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/preset-env": "^7.22.0",
    "eslint": "^9.34.0",
    "jest": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "vite": "^5.0.0",
    "vite-plugin-babel": "^1.3.2"
  },
  "dependencies": {
    "core-js": "^3.45.1",
    "pdfjs-dist": "^4.7.76",
    "regenerator-runtime": "^0.14.1",
    "tabulator-tables": "^5.4.4"
  }
}
